{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "saveinvoiceconfig"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "invoiceConfigBlob",
      "path": "outcontainer/invoiceConfig",
      "connection": "AzureWebJobsStorage"
    }
  ],
  "scriptFile": "../dist/SaveInvoiceConfig/index.js"
}
